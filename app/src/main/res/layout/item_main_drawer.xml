<?xml version="1.0" encoding="utf-8"?>
<com.cy.necessaryview.shapeview.RecShapeRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="50dp"
    android:padding="15dp"
    app:colorFill="@color/white"
    app:radiusCorner="12dp">

    <ImageView
        android:id="@+id/imgIcon"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:src="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/textName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="15dp"
        android:layout_toEndOf="@id/imgIcon"
        android:textColor="@color/main_drawer_item_text"
        android:textSize="16sp" />

</com.cy.necessaryview.shapeview.RecShapeRelativeLayout>